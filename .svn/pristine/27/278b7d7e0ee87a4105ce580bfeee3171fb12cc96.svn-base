package ua.nure.efimov.summarytask4.exception;

/**
 * Exception of Technical Layer of application(database, logger init, etc).
 * 
 * @author Alexandr Efimov
 *
 */
public class TechnicalException extends ApplicationException {

	/**
	 * SerialVersionUID.
	 */
	private static final long serialVersionUID = -1523663830184655662L;

	/**
	 * Constructs a new Technical Exception with {@code null} as its detail
	 * message.
	 * 
	 */
	public TechnicalException() {
	}

	public TechnicalException(String message) {
		super(message);
	}

	public TechnicalException(String message, Throwable cause) {
		super(message, cause);
	}

	public TechnicalException(Throwable cause) {
		super(cause);
	}
}
